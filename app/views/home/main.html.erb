<style type="text/css">
  .basic-card {
    height: 170px;
  }
  .basic-card-thumbnail {
    height: auto;
    min-height: 100% !important;
  }
 
</style>

<div class="row center">
  <p class="header col s12 light">오늘 할 덕질을 내일로 미루지 말자</p>
</div>
<div class="section">
  <div class="row">
    <% title = ["[1일 1덕]", "데일리 팬뉴스", "아티스트 프로필"] %>
    <% for i in 1..3 do %>
      <div class="col s12 m4">
        <div class="card">
          <div class="card-image basic-card">
            <img class="basic-card-thumbnail" src="images/basic<%= i %>.png">
            <span class="card-title"><%= title[i-1] %></span>
          </div>
        </div>
      </div>
    <% end %>
  </div>
</div>
<div class="section">
   
   <div class="row">
     <% for i in 1..6 do %>
        <div class="col s12 m4">
          <div class="card">
            <div class="card-image">
              <img src="images/test<%= i %>.jpg">
              <span class="card-title">Card Title</span>
            </div>
            <div class="card-content">
              <p>I am a very simple card. I am good at containing small bits of information.
              I am convenient because I require little markup to use effectively.</p>
            </div>
            <div class="card-action">
              <a href="#">This is a link</a>
            </div>
          </div>
        </div>
      <% end %>  
    </div>
        
</div>
